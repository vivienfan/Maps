    <div id = "loginmodal" class="modal fade login-modal" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel">
      <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
          <section id="login_container">
            <div>
              <label for="email_id" class ="labelLogEmail">Email id </label>
              <input class="login_email" type="email" name="email"> <br>
              <label for="password">Password</label>
              <input class="login_password" type="password" name="password"> <br>
              <button id = "login"> Login </button>
            </div>
          </section>'
        </div>
      </div>
    </div>

    <div id="regmodal" class="modal fade register-modal" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel">
      <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <section id="register_container">
              <div>
                <label for="email_id" class="labelRegEmail">Email id </label>
                <input class="reg_email" type="email" name="email"> <br>
                <label for="password">Password</label>
                <input class="reg_password" type="password" name="password"> <br>
                <label for="username">Username</label>
                <input class="reg_username" type="text" name="username"> <br>
                <button id="register">Register</button>
              </div>
            </section>
        </div>
      </div>
